# Anthropic API key for translations and AI features
ANTHROPIC_API_KEY=sk-ant-...

# Genius API token for song scraping
GENIUS_ACCESS_TOKEN=...

# Database URL (PostgreSQL for production, SQLite for local dev)
# Local: sqlite:///./ksl.db
# Production: postgresql://user:pass@host:5432/dbname
DATABASE_URL=sqlite:///./ksl.db

# AI model to use (claude-sonnet-4-20250514 or claude-haiku-4-5-20251001)
AI_MODEL=claude-haiku-4-5-20251001
